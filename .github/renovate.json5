{
  "regexManagers": [
      {
        "datasourceTemplate": "github-releases",
        "fileMatch": [
          "argocd-config/base/.*\\.yaml"
        ],
        "matchStrings": [
          " +targetRevision: +(?<currentValue>[^'\" ]+) +# renovate: depName=(?<depName>[^ \\n]+) +(extractVersion=(?<extractVersion>[^\\n]+))?",
        ],
      },
    ]
}
